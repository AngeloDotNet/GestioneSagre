{
    "Kestrel": {
        "Limits": {
            "MaxRequestBodySize": 5242880
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Warning"
        }
    },
    "ConnectionStrings": {
        "Default": "Data Source=NOME-SERVER\\NOME-ISTANZA;Initial Catalog=NOME-DATABASE;User ID=USERNAME;Password=PASSWORD"
    },
    "Database": {
        "maxRetryCount": 10,
        "maxRetryDelay": 30
    },
    "Swagger": {
        "enabled": true
    }
}